@model IEnumerable<projectpdf1.Models.Team>

@{
    ViewBag.Title = "ListProjectTL";
    Layout = "~/Views/Shared/_CustomerAndJE.cshtml";
}


<p>
    @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-warning" })
</p>

<table class="table">
    <tr>



        <th>
            @Html.DisplayNameFor(model => model.U_id_PM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.U_id_JE)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.U_id_TL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.From)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.To)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model)
        {

    <tr>

        <th>
            @Html.DisplayFor(model => item.U_id_PM)
        </th>
        <th>
            @Html.DisplayFor(model => item.U_id_JE)
        </th>

        <th>
            @Html.DisplayFor(model => item.U_id_TL)
        </th>

        <th>
            @Html.DisplayFor(model => item.From)
        </th>

        <th>
            @Html.DisplayFor(model => item.To)
        </th>

        <th>
            @Html.DisplayFor(model => item.Price)
        </th>

        <td>
            @Html.ActionLink("Details", "Details", new { id = item.project_id }) |
            @Html.ActionLink("LeaveProject", "LeaveProject", new { id = item.Team_Id }) |
            @Html.ActionLink("Feedback", "Feedback", new { id = item.Team_Id }) |
            @Html.ActionLink("Remove JE", "RemoveJE", new { id = item.Team_Id }) |
            @Html.ActionLink("Assign JE", "AssignJE", new { id = item.project_id })


        </td>
    </tr>
        }
    }



</table>

